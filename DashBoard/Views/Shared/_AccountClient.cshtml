@using Dashboard.DataDto.User

@model List<AccountDto>

<div class="card">
    <div class="card-body">
        <h4 class="card-title">Basic Table</h4>
        <table class="table">
            <thead>
                <tr>
                    <th>Tên tài khoản</th>
                    <th>Mật khẩu</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.UserName</td>
                        <td>@item.Password</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

<h1>Test client</h1>